#!/usr/bin/python
"""
What is the value of the first triangle number to have over five hundred
divisors?
"""

import math


def get_primes_upto(n):
    sieve = dict()
    sieve[0] = True
    sieve[1] = True
    for i in xrange(int(math.sqrt(n))):
        if i not in sieve:
            for j in xrange(i ** 2, n, i):
                sieve[j] = True
    return [i for i in xrange(2, n) if i not in sieve]


